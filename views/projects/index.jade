extends ../layout

block main
	ul.breadcrumb
		li
			i.icon-home
		li
			i.icon-angle-right
		li
			a(href="/") 面板


	div(class="row-fluid sortable")
		div(class="box span12")
			div(class="box-header", data-original-title)
				h2
					i(class="halflings-icon user")
					span.break
					| 项目

			div.box-content
				table(class="table table-striped table-bordered bootstrap-datatable datatable")
					thead
						tr
							th 序号
							th 级别
							th 金手指
							th 成就
							th 未解锁
					tbody
						- each project in projects
							tr
								td= project.get('sequence')
								td= project.get('level')
								td= project.get('tool_ratio')
								td= project.get('achieve')
								td= project.get('unlocked')
								
